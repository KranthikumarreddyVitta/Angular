{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bus": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/bus",
      "sourceRoot": "projects/bus/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bus",
            "index": "projects/bus/src/index.html",
            "main": "projects/bus/src/main.ts",
            "polyfills": "projects/bus/src/polyfills.ts",
            "tsConfig": "projects/bus/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/bus/src/favicon.ico",
              "projects/bus/src/assets"
            ],
            "styles": [
              "projects/bus/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/bus/src/environments/environment.ts",
                  "with": "projects/bus/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bus:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bus:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bus:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bus/src/test.ts",
            "polyfills": "projects/bus/src/polyfills.ts",
            "tsConfig": "projects/bus/tsconfig.spec.json",
            "karmaConfig": "projects/bus/karma.conf.js",
            "assets": [
              "projects/bus/src/favicon.ico",
              "projects/bus/src/assets"
            ],
            "styles": [
              "projects/bus/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/bus/tsconfig.app.json",
              "projects/bus/tsconfig.spec.json",
              "projects/bus/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/bus/e2e/protractor.conf.js",
            "devServerTarget": "bus:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bus:serve:production"
            }
          }
        }
      }
    },
